## http://editorconfig.org

## Top-most EditorConfig file
root = true

## UTF-8 charset, Unix-style newlines, double-space indent size,
#space-indents, a newline ending every file, trim off trailing whitespace
[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
